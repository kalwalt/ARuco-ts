    "jest": "^30.2.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
