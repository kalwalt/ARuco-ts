{
  "name": "aruco-ts",
  "version": "0.2.0",
  "description": "A TypeScript implementation of js-aruco, designed for developers interested in augmented reality and computer vision, providing tools for marker detection and tracking in WebAR applications.",
  "main": "dist/ARuco.js",
  "types": "types/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist ./types",
    "build-ts": "webpack --mode production",
    "dev-ts": "webpack --mode development --progress --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "bench": "npm run bench:compile && npm run bench:run",
    "bench:compile": "tsc benchmarks/math-performance.bench.ts --outDir .bench --module commonjs --target ES2020 --esModuleInterop --resolveJsonModule",
    "bench:run": "node .bench/benchmarks/math-performance.bench.js && rimraf ./.bench"
  },
  "keywords": [
    "ARuco",
    "Augmented-Reality",
    "OpenCV",
    "Typescript",
    "WebAR"
  ],
  "author": "Walter Perdan <github@kalwaltart.it>",
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "prettier": "^2.8.8",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.105.0",
    "webpack-cli": "^6.0.1"
  }
}
